#include<iostream>
using namespace std;
int main ()
{  
    int i, num, div, sum=0;
    cin >> num;
    for (i=1; i < num; i++)
    {
        div = num % i;
        if (div == 0)
            sum = sum + i;
    }
    if (sum == num)
        cout << "True";
    else
        cout << "False";
    return 0;
}
